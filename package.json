{
  "name": "RN-Jest",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios --no-packager --simulator='iPhone X'",
    "ios:legacy": "react-native run-ios --simulator='iPhone 8'",
    "dev": "concurrently 'yarn ios' 'yarn start --reset-cache'",
    "dev:nonconcurrent": "yarn ios && yarn start --reset-cache",
    "dev:android": "concurrently 'react-native run-android --no-packager' 'yarn start --reset-cache'",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "lint": "yarn run lint:ts && yarn run lint:prettier -l",
    "lint:ts": "tslint -p tsconfig.json",
    "lint:ts:fix": "yarn run lint:ts --fix",
    "lint:ts:compile": "tsc -p tsconfig.linting.json",
    "lint:fix": "yarn lint:ts:fix && yarn lint:prettier:fix"
  },
  "dependencies": {
    "@babel/preset-env": "^7.0.0",
    "@types/hoist-non-react-statics": "^3.0.1",
    "@types/lodash.orderby": "^4.6.4",
    "@types/lodash.throttle": "^4.1.4",
    "@types/node": "^10.12.0",
    "@types/ramda": "^0.25.39",
    "@types/react-native-auth0": "^1.2.5",
    "@types/react-native-custom-tabs": "^0.1.1",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-navigation": "^3.0.1",
    "@types/react-redux": "^6.0.9",
    "@types/redux-persist": "^4.3.1",
    "@types/styled-components": "^4.0.3",
    "@types/uuid": "^3.4.4",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-plugin-macros": "^2.4.2",
    "babel-preset-expo": "^5.0.0",
    "hoist-non-react-statics": "^3.0.1",
    "immer": "^1.7.2",
    "lodash.orderby": "^4.6.0",
    "lodash.throttle": "^4.1.1",
    "polished": "^2.2.0",
    "ramda": "^0.25.0",
    "react": "^16.8.3",
    "react-native": "^0.57.5",
    "react-native-custom-tabs": "^0.1.7",
    "react-native-elements": "^0.19.1",
    "react-native-fast-image": "^5.0.10",
    "react-native-gesture-handler": "^1.0.15",
    "react-native-iphone-x-helper": "^1.2.0",
    "react-native-mock-render": "^0.1.2",
    "react-native-picker-select": "^5.0.0",
    "react-native-progress": "^3.5.0",
    "react-native-scalable-image": "^0.4.0",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "^3.0.9",
    "react-navigation-current-route": "^1.0.0",
    "react-navigation-fluid-transitions": "^0.3.0",
    "react-navigation-props-mapper": "^0.3.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.14",
    "@types/react": "^16.4.18",
    "@types/react-native": "0.57.3",
    "@types/react-test-renderer": "^16.8.1",
    "babel-jest": "^23.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "^5",
    "concurrently": "^4.0.1",
    "danger": "^7.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^23.6.0",
    "jest-expo": "^32.0.0",
    "jest-junit": "^5.1.0",
    "jest-styled-components": "^6.1.1",
    "metro-react-native-babel-preset": "^0.48.1",
    "patch-package": "^5.1.1",
    "plist": "^3.0.1",
    "postinstall-prepare": "^1.0.1",
    "prettier": "^1.14.0",
    "react-native-testing-library": "^1.3.0",
    "react-native-version": "^2.6.5",
    "react-test-renderer": "^16.8.3",
    "ts-jest": "^24.0.0",
    "ts-mockery": "^1.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-no-circular-imports": "^0.6.1",
    "typescript": "^3.1.3"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "jest": {
    "resetMocks": true,
    "restoreMocks": true,
    "preset": "react-native",
    "testRegex": "\\.test\\.(ts|tsx)$",
    "transform": {
      "^.+\\.(j|t)sx?$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
